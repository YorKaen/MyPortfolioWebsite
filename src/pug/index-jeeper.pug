mixin menu (name, sub1, sub2, sub3, sub4)
	ul.sub-menu= name
		.arrow-down
			img(src="/img/jeeper/arrow-down.png", alt="")
		.menu-mode
			li
				a(href="#")
					img(src="/img/jeeper/654bhfhdgdf.png", alt="", srcset="")
					p= sub1
			li
				a(href="#")
					img(src="/img/jeeper/654bhfhdgdf.png", alt="", srcset="")
					p= sub2
			li
				a(href="#")
					img(src="/img/jeeper/654bhfhdgdf.png", alt="", srcset="")
					p= sub3
			li
				a(href="#")
					img(src="/img/jeeper/654bhfhdgdf.png", alt="", srcset="")
					p= sub4
			li
				a(href="#")
					img(src="/img/jeeper/654bhfhdgdf.png", alt="", srcset="")
					p= sub4
			li
				a(href="#")
					img(src="/img/jeeper/654bhfhdgdf.png", alt="", srcset="")
					p= sub4

mixin logo-slider(path,name,...MenuList)
		.logos-slider__slide
			.logos-slider__front
				img(src=path, alt="")
				p= name
			ul.logos-slider__menu
				li
						a(href= name)
							img(src=path, alt="", srcset="")
							span Все модели
				each item in MenuList
					- var patt = /\s+/i;
					if patt.test(item)
						- var newitem = item.split(' ').join('_');
							li
								a&attributes({ href: name + "#" + newitem.toLowerCase() + '.html' })
									img(src=path, alt="", srcset="")
									span= item
					else
						li
							a&attributes({ href: name + "#" + item.toLowerCase() + '.html' })
								img(src=path, alt="", srcset="")
								span= item

mixin accordeon(path,...MenuList)
				each item in MenuList
					.accordion-item__link
						a(href="#")
							img(src= path, alt="", srcset="")
							p= item

mixin f-list(name,...MenuList)
	each item in MenuList
		li
			a&attributes({ href: name + "#" + item.toLowerCase() + '.html' })=item

